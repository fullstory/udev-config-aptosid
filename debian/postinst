#! /bin/sh
set -e

rm -f	/etc/modprobe.d/ieee1394.conf		\
	/etc/modprobe.d/sidux.conf		\
	/etc/modprobe.d/kanotix			\
	/etc/modprobe.d/mac80211		\
	/etc/modprobe.d/openfwwf		\
	/etc/modprobe.d/openfwwf.conf		\
	/etc/udev/bluetooth.rules		\
	/etc/udev/elsa.rules			\
	/etc/udev/networkfix.rules		\
	/etc/udev/ttyACM.rules			\
	/etc/udev/hisax.rules			\
	/etc/udev/rules.d/010_networkfix.rules	\
	/etc/udev/rules.d/bluetooth.rules	\
	/etc/udev/rules.d/hisax.rules		\
	/etc/udev/rules.d/z80_elsa.rules	\
	/etc/udev/rules.d/z80_ttyACM.rules

# clean up eventually left over /usr/sbin/policy-rc.d, which might not have
# been cleaned up correctly by fll-installer << 0.5.12.
if [ -f /usr/sbin/policy-rc.d ] && \
   [ -z "$(dpkg-divert --list /usr/sbin/policy-rc.d)" ] && \
   [ "x$(md5sum -b /usr/sbin/policy-rc.d | awk '{print $1}')" = "x2f31905d39c6a4ed28a9217decbb7208" ]; then
	rm -f /usr/sbin/policy-rc.d
fi

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
