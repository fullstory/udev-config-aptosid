#! /bin/sh

rm -f	/etc/udev/elsa.rules \
	/etc/udev/rules.d/z80_elsa.rules \
    /etc/modprobe.d/kanotix

case "$1" in
	purge)
		rm -f	/etc/udev/networkfix.rules \
			/etc/udev/rules.d/010_networkfix.rules
		;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
